<%= form_with(model: @training, local: true) do |form| %>
    <%= render 'shared/error_messages' %>

    <div class="title">
      <%= form.label :タイトル %>
      <%= form.text_field :title %>
    </div>
  
    <div class="time">
      <%= form.label :スケジュール %>
      <%= form.datetime_select :start_time %>
    </div>
  
    <div class="content">
      <%= form.label :内容 %>
      <%= form.text_field :content %>
    </div>
  
  
    <div class="submit">
      <%= form.submit %>
      
    </div>
  
<% end %>
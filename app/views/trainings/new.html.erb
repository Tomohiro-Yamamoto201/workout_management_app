<%= form_with(model: [@training,@training_menu], local: true) do |form| %>

    <div class="title">
      <%= form.label :タイトル %> 
      <%= form.text_field :training_menu %>
    </div>
    #value: @training_menu.training_menu 

    <div class="time">
      <%= form.label :スケジュール %>
      <%= form.datetime_select :start_time %>
    </div>
  
    <div class="content">
      <%= form.label :内容 %>
      <%= form.text_field :content %>
    </div>
  
    <div class="growing_parts">
      <%= form.label :成長部位 %>
      <%= form.text_field :growing_parts %>
    </div>
  
  
    <div class="submit">
      <%= form.submit %>
      
    </div>
  
<% end %>